{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["sin1"],
  "env": {
    "PRISMA_GENERATE_DATAPROXY": "true"
  },
  "crons": [
    {
      "path": "/api/cron/auto-delete-threads",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/cleanup-user-statuses",
      "schedule": "0 2 * * *"
    },
    {
      "path": "/api/cron/schedule-reminder",
      "schedule": "0 11,14 * * *"
    }
  ],
  "headers": [
    {
      "source": "/TuntasinAja.apk",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/vnd.android.package-archive"
        },
        {
          "key": "Content-Disposition",
          "value": "attachment; filename=\"TuntasinAja.apk\""
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    },
    {
      "source": "/api/app/download",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/vnd.android.package-archive"
        },
        {
          "key": "Content-Disposition",
          "value": "attachment; filename=\"TuntasinAja.apk\""
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}
